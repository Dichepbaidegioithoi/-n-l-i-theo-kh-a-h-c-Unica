#include <stdio.h>
#include <math.h>
int main()
{
	int a,b,c;
	scanf("%d %d %d", &a, &b, &c);
	if(a>b && a>c)
	{
		printf("So lon nhat la %d",a);
	}
	else if(b>a && b>c)
	{
		printf("So lon nhat la %d",b);
	}
	else if(c>a && c>b)
	{
		printf("So lon nhat la %d",c);
	}else
	{ 
	printf("Khong co so lon nhat");
	}
	return 0;
}
